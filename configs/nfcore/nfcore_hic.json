{
    "input": "data/input/nfcore_hic_samplesheet.csv",
    "outdir": "data/scratch/nfcore_hic",
    "fasta": "data/output/preprocessing/assemblies/hg38_genome_assembly.fna",
    "chromosome_size": "data/output/preprocessing/chrom_sizes/GRCh38.chrom.sizes",
    "restriction_site": "^GATC,G^ANTC",
    "ligation_site": "GATCGATC,GANTGATC,GANTANTC,GATCANTC",
    "bin_size": "1000",
    "skip_maps": true,
    "skip_mcool": true,
    "skip_dist_decay": true,
    "skip_compartments": true,
    "skip_balancing": true,
    "split_fastq": true,
    "schema_ignore_params": "cluster-options,clusterOptions,genomes,project"
}
