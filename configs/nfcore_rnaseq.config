params {
    data_dir = 'data'

    input = "${data_dir}/input/rnaseq/nfcore_rnaseq_samplesheet.csv"
    outdir = "${data_dir}/output/nfcore_rnaseq/"
    aligner = 'star_salmon'
    pseudo_aligner = 'salmon'
    fasta = "${data_dir}/output/preprocessing/assemblies/hg38_genome_assembly.fna"
    gtf = "${data_dir}/output/preprocessing/annotations/hg38_gencode_v43.gtf"

    schema_ignore_params = 'data_dir,clusterOptions,project'
}
